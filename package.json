  },
  "dependencies": {
    "gsap": "^3.10.4",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "concurrently": "^7.2.2",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.1.1",
    "prettier": "^2.7.0",
    "tailwindcss": "^3.1.3",
    "vite": "^2.9.9"
  }
