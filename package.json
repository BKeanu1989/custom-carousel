{
  "name": "custom-carousel",
  "private": true,
  "version": "0.0.0",
  "files": [
    "dist"
  ],
  "main": "./dist/custom-carousel.umd.js",
  "module": "./dist/custom-carousel.es.js",
  "exports": {
    ".": {
      "import": "./dist/custom-carousel.es.js",
